div#mapContainer
  position: relative
  width: 100%
  height: 600px
  overflow: hidden
  border: 1px solid #40628a
div#mapDraggable
  position: relative
  width: 1200px
  height: 1200px
  margin: 100px
div#planetMap
  position: relative
  width: 600px
  height: 600px
  margin: auto
  overflow: hidden
  border: 1px solid #40628a
div#rangeCircle
  position:absolute
  border-radius: 50%
  border: 2px solid #336600
  width: 480px
  height: 480px
  margin-bottom: -240px
  margin-left: -240px
.star_marker
  display: inline-block
  padding: 0px 5px 0px 5px
  color: rgba(0,0,0,0)
  line-height: 25px
  width: 25px
  height: 25px
  margin-left: -12px
  margin-bottom: -12px
  border-radius: 50%
  background: radial-gradient(ellipse at center, rgba(255,255,255,1) 0%, rgba(255,255,255,1) 10%,rgba(255,255,255,0.71) 35%,rgba(255,255,255,0) 60%)
.star_marker_outline_green
  border: 1px solid #008000
.star_marker_outline_yellow
  border: 1px solid #A66300
.star_marker_outline_red
  border: 1px solid #A30000
.star_marker_outline_blue
  border: 1px solid #193A7A
.orbitCircle
  position: absolute
  border-radius: 50%
  border: 1px solid white
  left: 300px
  bottom: 300px